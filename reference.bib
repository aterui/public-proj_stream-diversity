@article{francisAquaticInsectsPlay2006,
  title = {Aquatic Insects Play a Minor Role in Dispersing Salmon-Derived Nutrients into Riparian Forests in Southwestern {{Alaska}}},
  author = {Francis, T. B. and Schindler, D. E. and Moore, J. W.},
  year = {2006},
  month = nov,
  volume = {63},
