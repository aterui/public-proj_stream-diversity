  keywords = {acclimation,AMMONIA,ANIMALS,COMMUNITIES,critical thermal maximum,DISSOLVED-OXYGEN,ECOSYSTEMS,gape behavior,hypoxia,JUVENILE,RECENT CLIMATE-CHANGE,RESPONSES,TEMPERATURE,thermal stress,traits,unionid,UNIONID MUSSELS},
