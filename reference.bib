@article{valiquetteLossGeneticIntegrity2014,
  title = {Loss of Genetic Integrity in Wild Lake Trout Populations Following Stocking: Insights from an Exhaustive Study of 72 Lakes from {{Qu\'ebec}}, {{Canada}}},
  author = {Valiquette, Eliane and Perrier, Charles and Thibault, Isabel and Bernatchez, Louis},
  year = {2014},
