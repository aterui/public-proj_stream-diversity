@article{kanoPresentStatusConservation2010,
  title = {Present Status and Conservation of the Markless Forms of Stream-Resident Masu Salmon {{Oncorhynchus}} Masou (the so-Called `Iwame') in {{Japanese}} Mountain Streams},
  author = {Kano, Yuichi and Kondou, Takuya and Shimizu, Yoshitaka},
