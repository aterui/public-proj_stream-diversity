  keywords = {AIR-TEMPERATURE,aquatic habitat,ATLANTIC SALMON PARR,BROWN TROUT,FORESTED RIPARIAN AREAS,GEOTHERMALLY HEATED STREAM,LAKE DISTRICT STREAM,model,river,SEA-TROUT FRY,SMALL MOUNTAIN,STREAMS,WASHINGTON,water temperature,WATER TEMPERATURE BEHAVIOR,WESTERN},
