@article{itakuraAnguillidEelsSurrogate2020,
  title = {Anguillid Eels as a Surrogate Species for Conservation of Freshwater Biodiversity in {{Japan}}},
  author = {Itakura, Hikaru and Wakiya, Ryoshiro and Gollock, Matthew and Kaifu, Kenzo},
  year = {2020},
