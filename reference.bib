@article{polceClimatedrivenSpatialMismatches2014,
  title = {Climate-Driven Spatial Mismatches between {{British}} Orchards and Their Pollinators: Increased Risks of Pollination Deficits},
  author = {Polce, Chiara and Garratt, Michael P. and Termansen, Mette and {Ramirez-Villegas}, Julian and Challinor, Andrew J. and Lappage, Martin G. and Boatman, Nigel D. and Crowe, Andrew and Endalew, Ayenew Melese and Potts, Simon G. and Somerwill, Kate E. and Biesmeijer, Jacobus C.},
