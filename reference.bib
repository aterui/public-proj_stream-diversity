@article{saboRoleDischargeVariation2010,
  title = {The Role of Discharge Variation in Scaling of Drainage Area and Food Chain Length in Rivers},
  author = {Sabo, J. L. and Finlay, J. C. and Kennedy, T. and Post, D. M.},
  year = {2010},
  month = nov,
  volume = {330},
  pages = {965--7},
  issn = {1095-9203 (Electronic) 0036-8075 (Linking)},
  doi = {10.1126/science.1196005},
  abstract = {Food chain length (FCL) is a fundamental component of food web structure. Studies in a variety of ecosystems suggest that FCL is determined by energy supply, environmental stability, and/or ecosystem size, but the nature of the relationship between environmental stability and FCL, and the mechanism linking ecosystem size to FCL, remain unclear. Here we show that FCL increases with drainage area and decreases with hydrologic variability and intermittency across 36 North American rivers. Our analysis further suggests that hydrologic variability is the mechanism underlying the correlation between ecosystem size and FCL in rivers. Ecosystem size lengthens river food chains by integrating and attenuating discharge variation through stream networks, thereby enhancing environmental stability in larger river systems.},
  journal = {Science},
  keywords = {*Ecosystem,*Food Chain,*Rivers,Animals,Environment,Fishes,Invertebrates,North America,Water Cycle,Water Movements},
  number = {6006}
}

