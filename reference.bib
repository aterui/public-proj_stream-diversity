@article{weberPhylogenyEcologyCoupling2012,
  title = {Phylogeny, Ecology, and the Coupling of Comparative and Experimental Approaches},
  author = {Weber, M. G. and Agrawal, A. A.},
  year = {2012},
  month = jul,
  volume = {27},
