@article{krausMetamorphosisAltersContaminants2014,
  title = {Metamorphosis Alters Contaminants and Chemical Tracers in Insects: Implications for Food Webs},
  author = {Kraus, J. M. and Walters, D. M. and Wesner, J. S. and Stricker, C. A. and Schmidt, T. S. and Zuellig, R. E.},
  year = {2014},
  month = sep,
  volume = {48},
  pages = {10957--65},
  issn = {1520-5851 (Electronic) 0013-936X (Linking)},
  doi = {10.1021/es502970b},
  abstract = {Insects are integral to most freshwater and terrestrial food webs, but due to their accumulation of environmental pollutants they are also contaminant vectors that threaten reproduction, development, and survival of consumers. Metamorphosis from larvae to adult can cause large chemical changes in insects, altering contaminant concentrations and fractionation of chemical tracers used to establish contaminant biomagnification in food webs, but no framework exists for predicting and managing these effects. We analyzed data from 39 studies of 68 analytes (stable isotopes and contaminants), and found that metamorphosis effects varied greatly. delta(15)N, widely used to estimate relative trophic position in biomagnification studies, was enriched by approximately 1 per thousand during metamorphosis, while delta(13)C used to estimate diet, was similar in larvae and adults. Metals and polycyclic aromatic hydrocarbons (PAHs) were predominantly lost during metamorphosis leading to approximately 2 to 125-fold higher larval concentrations and higher exposure risks for predators of larvae compared to predators of adults. In contrast, manufactured organic contaminants (such as polychlorinated biphenyls) were retained and concentrated in adults, causing up to approximately 3-fold higher adult concentrations and higher exposure risks to predators of adult insects. Both food web studies and contaminant management and mitigation strategies need to consider how metamorphosis affects the movement of materials between habitats and ecosystems, with special regard for aquatic-terrestrial linkages.},
  file = {C\:\\Users\\Akira\\OneDrive - UNCG\\Documents\\zotero_pdf\\Kraus et al_2014_Metamorphosis alters contaminants and chemical tracers in insects.pdf},
  journal = {Environmental science \& technology},
  keywords = {*Food Chain,Animals,Carbon Isotopes/analysis/pharmacokinetics,Environmental Pollutants/analysis/*pharmacokinetics,Insects/*chemistry/physiology,Isotope Labeling,Metals; Heavy/analysis/pharmacokinetics,Metamorphosis; Biological/*physiology,Nitrogen Isotopes/analysis/pharmacokinetics,Polychlorinated Biphenyls/analysis/pharmacokinetics,Polycyclic Hydrocarbons; Aromatic/analysis/pharmacokinetics,Regression Analysis},
  number = {18}
}

@article{krebsDeterminantsMusselDiversity2010,
  title = {Determinants of Mussel Diversity in {{Lake Erie}} Tributaries},
  author = {Krebs, R. A. and Borden, W. C. and Steiner, E. R. and Lyons, M. S. and Zawiski, W. and Walton, B. M.},
  year = {2010},
  month = jun,
  volume = {29},
  pages = {506--520},
  abstract = {We examined recent faunal changes in freshwater mussels (Unionidae) in northern Ohio. We assessed species diversity in 2 agricultural watersheds, the Vermilion and Huron Rivers, and compared these faunas among 7 neighboring rivers within the Lake Erie watershed that varied in levels of human impact, geological structure, and fish diversity. We found 19 mussel species in each of the Huron and Vermilion Rivers, and 89\% of species overlapped between the 2 rivers. Museum records suggested that as many as 4 species might now be missing from each river, and across the 9 tributaries in the region, species loss has been common. The total number of species once present in each river was positively correlated with watershed area, but the size of watersheds no longer provides a significant estimate of mussel or fish diversity. Current mussel faunas also were weakly correlated with the fish species present, suggesting that mussel species ranges have not been limited by a lack of available fish hosts.},
  journal = {Journal of the North American Benthological Society},
  keywords = {biodiversity,coastal wetland,conservation,fish,Fresh-water mussels,freshwater mussels,grand-river,macroinvertebrate diversity,north-america,species area curves,species richness,streams,unionid bivalves,Unionidae,vegetation,zebra mussel},
