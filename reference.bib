@article{araujoUsingSpeciesCooccurrence2011,
  title = {Using Species Co-Occurrence Networks to Assess the Impacts of Climate Change},
  author = {Ara{\'u}jo, Miguel B. and Rozenfeld, Alejandro and Rahbek, Carsten and Marquet, Pablo A.},
