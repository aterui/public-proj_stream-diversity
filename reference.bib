@article{sugiyamaHabitatSelectionLarvae2002,
  title = {Habitat Selection by Larvae of a Fluvial Lamprey, {{{\emph{Lethenteron}}}}{\emph{ Reissneri}}, in a Small Stream and an Experimental Aquarium},
  author = {Sugiyama, H. and Goto, A.},
