@article{hastieTimingSpawningGlochidial2003,
  title = {Timing of Spawning and Glochidial Release in {{Scottish}} Freshwater Pearl Mussel ({{{\emph{Margaritifera}}}}{\emph{ Margaritifera}}) Populations},
  author = {Hastie, L. C. and Young, M. R.},
