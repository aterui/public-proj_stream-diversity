@article{oliverRabbitsEatVoles2009,
  title = {Do Rabbits Eat Voles? {{Apparent}} Competition, Habitat Heterogeneity and Large-Scale Coexistence under Mink Predation},
  author = {Oliver, M. and {Luque-Larena}, J. J. and Lambin, X.},
  year = {2009},
